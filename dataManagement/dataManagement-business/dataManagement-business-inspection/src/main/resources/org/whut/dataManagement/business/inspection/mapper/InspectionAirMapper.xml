<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.whut.dataManagement.business.inspection.mapper.InspectionAirMapper">
    <select id="findAirByCode" parameterType="java.lang.String" resultType="java.lang.String">
        SELECT FPressure
        FROM tb_NLCheck
        WHERE FMeterCode = #{fmetercode} ORDER BY FID;
    </select>
</mapper>